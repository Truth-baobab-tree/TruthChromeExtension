{
  "name": "Is this the truth?",
  "key": "",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "128": "./public/icons/icon.png"
  },
  "description": "It is an extension program that helps users determine whether the information on the website they are currently accessing is true.",
  "background": {
    "scripts": [
      "./script/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Is this the truth?",
    "default_icon": "./public/icons/icon.png",
    "default_popup": "./src/pages/Main/index.html"
  },
  "author": "김형진 & 강태영",
  "permissions": [
    "tabs",
    "activeTab"
 ]
}